<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.






    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
    </style>

    <style name="AppTheme" parent="AppBaseTheme"></style>

    <style name="title_text_styling">
        <item name="android:typeface">normal</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="MyRadioButtonStyle" parent="@android:style/Widget.CompoundButton.RadioButton">
        <item name="android:button">@drawable/custom_btn_radio</item>
    </style>

    <style name="footer_button" parent="title_text_styling">
        <item name="android:paddingTop">6dp</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">12dp</item>
        <item name="android:onClick">onToggle</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@drawable/menu_footer_text_color</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="footer_button_new" parent="title_text_styling">
        <item name="android:paddingTop">6dp</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">12dp</item>
        <item name="android:onClick">onToggle</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@drawable/menu_footer_text_color_new</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="Theme.D1NoTitleDim" parent="android:style/Theme.Translucent">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:background">@android:color/transparent</item>
    </style>


    <style name="CustomDialogTheme" parent="@android:style/Theme.Light.Panel">
        <item name="android:background">@android:color/white</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="AlertDialogCustom" parent="@android:style/Theme.Holo.Light.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:textAppearance">@color/black</item>
        <item name="android:windowTitleStyle">@style/Dialog</item>
    </style>

    <style name="StyledIndicators" parent="@android:style/Theme.NoTitleBar">
        <item name="vpiCirclePageIndicatorStyle">@style/CustomCirclePageIndicator</item>
    </style>

    <style name="CustomCirclePageIndicator">
        <item name="fillColor">#000000</item>
        <item name="strokeColor">#000000</item>
        <item name="strokeWidth">1dp</item>
        <item name="radius">6dp</item>
        <item name="centered">true</item>
    </style>
    <style name="Dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowTitleStyle">@style/MyOwnDialogTitle</item>
    </style>

    <style name="MyOwnDialogTitle">
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="more_item_layout" parent="title_text_styling">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">10dp</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:onClick">goTo</item>
        <item name="android:background">@android:color/transparent</item>
    </style>
    <style name="more_separator" parent="title_text_styling">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">10dp</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:onClick">goTo</item>
        <item name="android:background">@android:color/transparent</item>
    </style>

    <style name="more_image">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:padding">4dp</item>
        <item name="android:background">@android:color/transparent</item>
    </style>

    <style name="more_image_ar">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:padding">4dp</item>
        <item name="android:background">@android:color/transparent</item>
    </style>

    <style name="more_bullet">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:src">@drawable/arrow</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:padding">4dp</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:layout_marginRight">7dp</item>
    </style>

    <style name="more_bullet_ar">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:src">@drawable/arrow</item>
        <item name="android:rotation">180</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:padding">4dp</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:layout_marginLeft">7dp</item>
    </style>

    <style name="more_text" parent="title_text_styling">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:padding">2dp</item>
        <item name="android:background">@android:color/transparent</item>
    </style>

    <style name="more_text_ar" parent="title_text_styling">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:gravity">right|center_vertical</item>
        <item name="android:padding">2dp</item>
        <item name="android:background">@android:color/transparent</item>
    </style>


    <style name="settings_separator">
        <item name="android:layout_height">1dp</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:background">@color/white</item>
        <item name="android:textSize">20dp</item>
    </style>
    <style name="settings_separator2">
        <item name="android:layout_height">15dp</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:background">@color/white</item>
        <item name="android:textSize">20dp</item>
    </style>
    <style name="tableRow">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:background">@color/white</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="tableRowTitle">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:background">@color/night_gray</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:padding">5dp</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="tableRowTitle2">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:background">@color/white</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:padding">5dp</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="tableRowSeparator">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:background">@color/night_gray</item>
    </style>

    <style name="edittext">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:background">@drawable/edittext_bg</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:maxLines">1</item>
        <item name="android:textSize">13dp</item>
    </style>

    <style name="formLabel">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:textSize">13dp</item>
    </style>

    <style name="formLabelType">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:text">*</item>
        <item name="android:textColor">@color/red</item>
        <item name="android:textSize">13dp</item>
    </style>

    <style name="speedTestFormLabel">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">@color/gray</item>
        <item name="android:textSize">13dp</item>
    </style>

    <style name="speedTestFormValue">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">@color/green</item>
        <item name="android:textSize">12dp</item>
    </style>


    <style name="map_spinner">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:background">@drawable/spinner_bg</item>
    </style>





</resources>